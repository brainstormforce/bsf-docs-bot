	"description": "Astra's Docs Bot.",
	"main": "",
	"scripts": {
		"build": "wp-scripts build",
		"start": "wp-scripts start",
		"i18n": "grunt textdomain && wp i18n make-pot languages/wp-plugin-base.pot --domain=wp-plugin-base --debug",
		"lint-js": "wp-scripts lint-js",
